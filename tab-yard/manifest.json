{
  "manifest_version": 3,
  "name": "KATIE'S TAB YARD",
  "version": "3.2.1",
  "description": "Import or capture tabs, then surface a random one to visit.",
  "permissions": ["tabs", "storage", "activeTab"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "KATIE'S TAB YARD",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
}
